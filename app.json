{
  "expo": {
    "name": "AquaSprout",
    "slug": "AquaSprout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/planta.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/pexels-scottwebb-1048035.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/planta.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mr.jonas.AquaSprout"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/planta.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "src/assets/fonts/BlackOpsOne-Regular.ttf",
            "src/assets/fonts/Oswald-VariableFont_wght.ttf"
          ]
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b3d8615-1a73-412d-b796-cdcb923cae59"
      }
    }
  }
}
